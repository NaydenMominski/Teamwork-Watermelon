 <div class="main-container col2-left-layout">
            <div class="main container">
                <div class="row">
                    <section class="col-sm-9 col-sm-push-3">
                        <div class="col-main">
                            <div class="category-title">
                                <h1>Tops &amp; Tees</h1>
                            </div>
                            <div class="category-description std">
                                <div class="slider-items-products">
                                    <div id="category-desc-slider" class="product-flexslider hidden-buttons">

                                    </div>
                                </div>
                            </div>
                            <div class="category-products">
                                <div class="toolbar">
                                    <div class="sorter">
                                        <div class="view-mode"> <span title="Grid" class="button button-active button-grid">Grid</span><a href="#" title="List" class="button button-list">List</a> </div>
                                    </div>
                                    <div id="sort-by">
                                        <label class="left">Sort By: </label>
                                        <ul>
                                            <li><a href="#">Position<span class="right-arrow"></span></a>
                                                <ul>
                                                    <li><a href="#">Name</a></li>
                                                    <li><a href="#">Price</a></li>
                                                    <li><a href="#">Position</a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                        <a class="button-asc left" href="#" title="Set Descending Direction"><span style="color:#999;font-size:11px;" class="glyphicon glyphicon-arrow-up"></span></a> </div>
                                    <div class="pager">
                                        <div id="limiter">
                                            <label>View: </label>
                                            <ul>
                                                <li><a href="#">15<span class="right-arrow"></span></a>
                                                    <ul>
                                                        <li><a href="#">20</a></li>
                                                        <li><a href="#">30</a></li>
                                                        <li><a href="#">35</a></li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="pages">
                                            <label>Page:</label>
                                            <ul class="pagination">
                                                <li><a href="#">&laquo;</a></li>
                                                <li class="active"><a href="#">1</a></li>
                                                <li><a href="#">2</a></li>
                                                <li><a href="#">3</a></li>
                                                <li><a href="#">&raquo;</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <ul id="products-image" class="products-grid">
                                    {{#this}}
                                    <li class="item col-lg-4 col-md-3 col-sm-4 col-xs-12">
                                        <div class="item-inner">
                                            <div class="product-block">
                                                <div id="product-image" class="product-image">
                                                    {{!--<a href="#">--}}
                                                        <figure data-creator="{{_acl.creator}}" data-id="{{_id}}" class="product-display">
                                                        {{#if url}}
                                                           <a href="#/books/{{_id}}">
                                                            <img  src="{{url}}" class="lazyOwl product-mainpic" alt="{{title}}" style="display: block;">
                                                            <img   class="product-secondpic" alt="{{title}}" src="{{url}}">
                                                         </a>
                                                         {{else}}
                                                          <a href="#/books/{{_id}}">
                                                            <img  src="../products-images/product1.jpg" class="lazyOwl product-mainpic" alt="{{title}}" style="display: block;">
                                                            <img   class="product-secondpic" alt="{{title}}" src="../products-images/product1.jpg">
                                                         </a>
                                                          {{/if}}
                                                        
                                                        </figure>
                                                    </a>
                                                </div>
                                                <div class="product-meta ">
                                                    <div class="product-action ">
                                                        <a  class="addcart " href="#"> <i class="icon-shopping-cart ">&nbsp;</i> Add to cart </a>

                                                        <a class="wishlist " href="#"> <i class="icon-heart ">&nbsp;</i> </a>

                                                        <a class="quickview " href="# "> <i class="icon-zoom ">&nbsp;</i> </a>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="item-info ">
                                                <div class="info-inner ">
                                                    <div class="item-title "> <a href="#" title="Retis lapen casen "> {{title}}</a> </div>
                                                    <div class="item-content ">

                                                        <div class="item-price ">
                                                            <div class="price-box "> <span class="regular-price "> <span class="price ">${{price}}</span> </span>
                                                            </div>
                                                        </div>
                                                        <div class="rating ">
                                                            <div class="ratings ">
                                                                <div class="rating-box ">
                                                                    <div class="rating " style="width:80% "></div>
                                                                </div>
                                                                <p class="rating-links "> <a href="# ">1 Review(s)</a> <span class="separator ">|</span> <a href="# ">Add Review</a> </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                 {{/this}}
                                </ul>
                            </div>
                        </div>
                    </section>
                    <aside class="col-left sidebar col-sm-3 col-xs-12 col-sm-pull-9 wow bounceInUp animated">
                        <div class="side-nav-categories">
                            <div class="block-title"> Categories </div>
                            <div class="box-content box-category">
                                <ul id="magicat">
                                    <li class="level0- level0 open"> <span class="magicat-cat"><a href="#"><span>Women</span></a>
                                        </span>

                                    </li>
                                    <li class="level0- level0"> <span class="magicat-cat"><a href="#l"><span>Men</span></a>
                                        </span>
                                    </li>
                                    <li class="level0- level0  shoes"> <span class="magicat-cat"><a href="#"><span>Shoes</span></a>
                                        </span>
                                    </li>
                                    <li class="level0- level0  watches open"> <span class="magicat-cat"><a href="#"><span>Watches</span></a>
                                        </span>

                                    </li>
                                    <li class="level0- level0 last"> <span class="magicat-cat"><a href="#"><span>Furniture</span></a>
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </aside>
                </div>
            </div>
        </div>